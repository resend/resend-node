  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "18.16.2",
    "@types/node-fetch": "2.6.4",
    "@types/react": "18.2.0",
    "fetch-mock": "9.11.0",
    "jest": "29.6.1",
    "jest-fetch-mock": "3.0.3",
    "prettier": "3.0.0",
